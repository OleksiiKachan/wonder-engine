    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.1",
    "commitlint": "^15.0.0",
    "cz-customizable": "^6.3.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
