{
  "name": "wonder-engine",
  "version": "0.0.14",
  "description": "Engine for ui kits and design systems",
  "main": "dist/wonder-engine.js",
  "typings": "dist/types/index.d.ts",
  "repository": "https://github.com/OleksiiKachan/wonder-engine",
  "author": "Oleksii Kachan",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "build": "yarn build:types && yarn build:package",
    "build:types": "yarn tsc",
    "build:package": "NODE_ENV=production webpack",
    "tsc": "NODE_ENV=production tsc --emitDeclarationOnly --declaration",
    "test": "jest --passWithNoTests",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": ">=5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@storybook/addon-actions": "^6.3.5",
    "@storybook/addon-docs": "^6.3.5",
    "@storybook/addon-essentials": "^6.3.5",
    "@storybook/addon-links": "^6.3.5",
    "@storybook/react": "^6.3.5",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.11",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.1",
    "commitlint": "^12.1.4",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "path": "^0.12.7",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".commitizen.json"
    }
  },
  "dependencies": {}
}
