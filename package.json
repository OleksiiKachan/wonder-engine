{
  "name": "wonder-engine",
  "version": "0.0.15",
  "description": "Engine for ui kits and design systems",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/wonder-engine.esm.js",
  "size-limit": [
    {
      "path": "dist/wonder-engine.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/wonder-engine.esm.js",
      "limit": "10 KB"
    }
  ],
  "files": [
    "dist"
  ],
  "repository": "https://github.com/OleksiiKachan/wonder-engine",
  "author": "Oleksii Kachan",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "husky install && tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": ">=5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@size-limit/preset-small-lib": "^5.0.1",
    "@storybook/addon-actions": "^6.3.5",
    "@storybook/addon-docs": "^6.3.5",
    "@storybook/addon-essentials": "^6.3.5",
    "@storybook/addon-links": "^6.3.5",
    "@storybook/react": "^6.3.5",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.11",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "commitlint": "^13.1.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "path": "^0.12.7",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^5.0.1",
    "styled-components": "^5.3.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".commitizen.json"
    }
  }
}
