    "jest": "^27.3.1",
    "microbundle": "^0.14.2",
    "path": "^0.12.7",
    "prettier": "^2.5.0",
    "pretty-quick": "^3.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.2"
